{
  "name": "holochain-rad-tools-phase-2",
  "version": "0.1.0",
  "description": "The second tier of RAD tools, automating a UI GraphQL and DNA generation based on a JSON schema file.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "generate:ui": "node setup/ui-setup/scripts/generate-ui",
    "ui:generate-clean": "rm -rf ui-src/*",
    "ui:connect-to-conductor": "cd ui-src && node connect-to-conductor",
    "ui:start-live": "cd ui-src && npm start:live",
    "hc-generate:dna": "node setup/dna-setup/scripts/generate-dna",
    "hc-generate:new-zome": "node setup/dna-setup/scripts/generate-zome",
    "hc-generate:new-zome-entry": "node setup/dna-setup/scripts/generate-zome-entry",
    "hc-keygen": "node setup/agent-setup/scripts/create-new-agent",
    "hc-config:add-agent": "node setup/agent-setup/add-agent-to-conductor",
    "hc-test": "cd dna-src && hc test",
    "hc-package": "cd dna-src && hc package",
    "hc-start": "holochain -c conductor-config.toml"
  },
  "devDependencies": {},
  "dependencies": {
    "ncp": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/holochain/RAD-Tools-Phase-2.git"
  },
